#ifndef __GNET_SETNEWROLEDETAILEXTENDRES_RPCDATA
#define __GNET_SETNEWROLEDETAILEXTENDRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class SetNewRoleDetailExtendRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		SetNewRoleDetailExtendRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		SetNewRoleDetailExtendRes(const SetNewRoleDetailExtendRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new SetNewRoleDetailExtendRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const SetNewRoleDetailExtendRes *r = dynamic_cast<const SetNewRoleDetailExtendRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		SetNewRoleDetailExtendRes& operator = (const SetNewRoleDetailExtendRes &rhs)
		{
			const SetNewRoleDetailExtendRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
