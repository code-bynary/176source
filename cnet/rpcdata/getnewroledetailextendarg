#ifndef __GNET_GETNEWROLEDETAILEXTENDARG_RPCDATA
#define __GNET_GETNEWROLEDETAILEXTENDARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class GetNewRoleDetailExtendArg : public GNET::Rpc::Data
	{
	public:
		unsigned int roleid;

	public:
		GetNewRoleDetailExtendArg (unsigned int l_roleid = 0)
			: roleid(l_roleid)
		{
		}

		GetNewRoleDetailExtendArg(const GetNewRoleDetailExtendArg &rhs)
			: roleid(rhs.roleid) { }

		Rpc::Data *Clone() const { return new GetNewRoleDetailExtendArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const GetNewRoleDetailExtendArg *r = dynamic_cast<const GetNewRoleDetailExtendArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		GetNewRoleDetailExtendArg& operator = (const GetNewRoleDetailExtendArg &rhs)
		{
			const GetNewRoleDetailExtendArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

	};
};
#endif
