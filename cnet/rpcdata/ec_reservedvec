#ifndef __GNET_EC_RESERVEDVEC_RPCDATA
#define __GNET_EC_RESERVEDVEC_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class EC_ReservedVec : public GNET::Rpc::Data
	{
	public:
		unsigned char reserved0;
		unsigned int reserved1;
		unsigned int reserved2;
		unsigned int reserved3;
		unsigned int reserved4;
		unsigned int reserved5;
		long long reserved6;
		unsigned int reserved7;
		unsigned int reserved8;

	public:
		EC_ReservedVec (unsigned char l_reserved0 = 0,unsigned int l_reserved1 = 0,unsigned int l_reserved2 = 0
			,unsigned int l_reserved3 = 0,unsigned int l_reserved4 = 0,unsigned int l_reserved5 = 0
			,long long l_reserved6 = 0,unsigned int l_reserved7 = 0,unsigned int l_reserved8 = 0)
			: reserved0(l_reserved0),reserved1(l_reserved1),reserved2(l_reserved2)
			,reserved3(l_reserved3),reserved4(l_reserved4),reserved5(l_reserved5)
			,reserved6(l_reserved6),reserved7(l_reserved7),reserved8(l_reserved8)
		{
		}

		EC_ReservedVec(const EC_ReservedVec &rhs)
			: reserved0(rhs.reserved0),reserved1(rhs.reserved1),reserved2(rhs.reserved2),
			reserved3(rhs.reserved3),reserved4(rhs.reserved4),reserved5(rhs.reserved5),
			reserved6(rhs.reserved6),reserved7(rhs.reserved7),reserved8(rhs.reserved8) { }

		Rpc::Data *Clone() const { return new EC_ReservedVec(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const EC_ReservedVec *r = dynamic_cast<const EC_ReservedVec *>(&rhs);
			if (r && r != this)
			{
				reserved0 = r->reserved0;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
				reserved5 = r->reserved5;
				reserved6 = r->reserved6;
				reserved7 = r->reserved7;
				reserved8 = r->reserved8;
			}
			return *this;
		}

		EC_ReservedVec& operator = (const EC_ReservedVec &rhs)
		{
			const EC_ReservedVec *r = &rhs;
			if (r && r != this)
			{
				reserved0 = r->reserved0;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
				reserved5 = r->reserved5;
				reserved6 = r->reserved6;
				reserved7 = r->reserved7;
				reserved8 = r->reserved8;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << reserved0;
			os << reserved1;
			os << reserved2;
			os << reserved3;
			os << reserved4;
			os << reserved5;
			os << reserved6;
			os << reserved7;
			os << reserved8;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> reserved0;
			os >> reserved1;
			os >> reserved2;
			os >> reserved3;
			os >> reserved4;
			os >> reserved5;
			os >> reserved6;
			os >> reserved7;
			os >> reserved8;
			return os;
		}

	};
	typedef GNET::RpcDataVector<EC_ReservedVec>	EC_ReservedVecVector;
};
#endif
