	public:
		int roleid;
		int cls;
		Octets name;
		IntVector members;
		int team_id;
		int team_type;
		int team_score;
		enum { PROTOCOL_TYPE = PROTOCOL_EC_ARENAENTERQUEUE };
	public:
		EC_ArenaEnterQueue() { type = PROTOCOL_EC_ARENAENTERQUEUE; }
		EC_ArenaEnterQueue(void*) : Protocol(PROTOCOL_EC_ARENAENTERQUEUE) { }
		EC_ArenaEnterQueue (int l_roleid = 0,int l_cls = 0,const Octets& l_name = Octets(0),
			const IntVector& l_members = IntVector(),int l_team_id = 0,int l_team_type = 0,
			int l_team_score = 0)
			 : roleid(l_roleid),cls(l_cls),name(l_name)
			,members(l_members),team_id(l_team_id),team_type(l_team_type)
			,team_score(l_team_score)
		{
			type = PROTOCOL_EC_ARENAENTERQUEUE;
		}

		EC_ArenaEnterQueue(const EC_ArenaEnterQueue &rhs)
			: Protocol(rhs),roleid(rhs.roleid),cls(rhs.cls),name(rhs.name)
			,members(rhs.members),team_id(rhs.team_id),team_type(rhs.team_type)
			,team_score(rhs.team_score) { }

		GNET::Protocol *Clone() const { return new EC_ArenaEnterQueue(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << cls;
			os << name;
			os << members;
			os << team_id;
			os << team_type;
			os << team_score;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> cls;
			os >> name;
			os >> members;
			os >> team_id;
			os >> team_type;
			os >> team_score;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 128; }
